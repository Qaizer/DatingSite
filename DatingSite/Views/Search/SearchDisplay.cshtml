@using DataAccessLayer
@using DatingSite.Controllers
@model DatingSite.Models.SearchModel

@{
    ViewBag.Title = "SearchDisplay";
}

<h2>SearchDisplay</h2>

<div id="searchresults">
    <ul>
        @foreach (var result in Model.SearchResult)
        {
        <li>@Html.ActionLink(result.Username, "Index", "Profile", new RouteValueDictionary(new { username = result.Username }), null)</li>
        }
    </ul>
</div>

